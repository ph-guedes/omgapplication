<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/panel_navigation"
    app:startDestination="@+id/navigation_addAnimal">

    <activity
        android:id="@+id/navigation_back"
        android:name="com.example.omgapplication.MainActivity"
        tools:layout="@layout/activity_main" />

    <fragment
        android:id="@+id/navigation_addAnimal"
        android:name="com.example.omgapplication.ui.panel.AddAnimalFragment"
        tools:layout="@layout/fragment_add_animal" />

    <fragment
        android:id="@+id/navigation_viewReport"
        android:name="com.example.omgapplication.ui.panel.ViewReportFragment"
        tools:layout="@layout/fragment_view_report" />

</navigation>